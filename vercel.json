{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
        "src": "/generateReport",
        "dest": "index.js"
        },
        {
        "src": "/(.*)",
        "dest": "index.js"
        }
    ]           
}
